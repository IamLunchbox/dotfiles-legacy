configuration {
        modi: "drun,window";
        show-icons: true;
        terminal: "i3-sensible-terminal";
        fixed-num-lines: true;
        sidebar-mode: true;
        kb-mode-next: "Tab";
        kb-row-tab: "";
        window-format: "{w}\t{c}";
        kb-cancel: "Alt+Tab,Escape";
}
@theme "themes/squared-nord"
